<script type="text/javascript">
$.urlParam = function(name){
    var results = new RegExp('[\\?&amp;]' + name + '=([^&amp;#]*)').exec(window.location.href);
    return results[1] || 0;
}
    window.onload = function(){
        $('#inputvalueID').val(decodeURIComponent($.urlParam('firstparameter')));
		   $('#divID').html(decodeURIComponent($.urlParam('secondparameter')));
};
</script>
